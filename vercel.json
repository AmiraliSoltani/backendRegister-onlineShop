{
    "version": 2,
    "routes": [
      {
        "src": "/api/register",
        "dest": "/api/register.js"
      },
      {
        "src": "/api/login",
        "dest": "/api/login.js"
      },
      {
        "src": "/api/lastSearches",
        "dest": "/api/lastSearches.js"
      },
      {
        "src": "/api/replaceLastSearches",
        "dest": "/api/replaceLastSearches.js"
      },
      {
        "src": "/api/shoppingCard",
        "dest": "/api/shoppingCard.js"
      },
      {
        "src": "/api/lastVisited",
        "dest": "/api/lastVisited.js"
      },
      {
        "src": "/api/replaceLastVisited",
        "dest": "/api/replaceLastVisited.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api/lastVisited/:id",
        "destination": "/api/lastVisited/[id].js"
      }
    ],
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ]
  }
  