{
    "version": 2,
    "builds": [
      {
        "src": "app.js",  
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",  
        "dest": "/app.js"  
      }
    ],
    "env": {
      "MONGODB_URI": "your_mongodb_connection_string_here",
      "JWT_SECRET": "your_jwt_secret_here",
      "CLIENT_ID": "your_google_oauth_client_id_here",
      "CLIENT_SECRET": "your_google_oauth_client_secret_here",
      "PORT": "8080"
    }
  }
  