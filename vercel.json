{
    "version": 2,
    "routes": [
      {
        "src": "/api/register",
        "dest": "/api/register.js"
      },
      {
        "src": "/api/login",
        "dest": "/api/login.js"
      }
    ],
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ]
  }
  